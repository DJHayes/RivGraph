@article{ilicak2016,
  title = {An Assessment of the {{Arctic Ocean}} in a Suite of Interannual {{CORE}}-{{II}} Simulations. {{Part III}}: {{Hydrography}} and Fluxes},
  shorttitle = {An Assessment of the {{Arctic Ocean}} in a Suite of Interannual {{CORE}}-{{II}} Simulations. {{Part III}}},
  author = {Il{\i}cak, Mehmet and Drange, Helge and Wang, Qiang and Gerdes, R{\"u}diger and Aksenov, Yevgeny and Bailey, David and Bentsen, Mats and Biastoch, Arne and Bozec, Alexandra and B{\"o}ning, Claus and Cassou, Christophe and Chassignet, Eric and Coward, Andrew C. and Curry, Beth and Danabasoglu, Gokhan and Danilov, Sergey and Fernandez, Elodie and Fogli, Pier Giuseppe and Fujii, Yosuke and Griffies, Stephen M. and Iovino, Doroteaciro and Jahn, Alexandra and Jung, Thomas and Large, William G. and Lee, Craig and Lique, Camille and Lu, Jianhua and Masina, Simona and George Nurser, A. J. and Roth, Christina and {Salas y M{\'e}lia}, David and Samuels, Bonita L. and Spence, Paul and Tsujino, Hiroyuki and Valcke, Sophie and Voldoire, Aurore and Wang, Xuezhu and Yeager, Steve G.},
  year = {2016},
  month = apr,
  volume = {100},
  pages = {141--161},
  issn = {1463-5003},
  doi = {10.1016/j.ocemod.2016.02.004},
  abstract = {In this paper we compare the simulated Arctic Ocean in 15 global ocean\textendash sea ice models in the framework of the Coordinated Ocean-ice Reference Experiments, phase II (CORE-II). Most of these models are the ocean and sea-ice components of the coupled climate models used in the Coupled Model Intercomparison Project Phase 5 (CMIP5) experiments. We mainly focus on the hydrography of the Arctic interior, the state of Atlantic Water layer and heat and volume transports at the gateways of the Davis Strait, the Bering Strait, the Fram Strait and the Barents Sea Opening. We found that there is a large spread in temperature in the Arctic Ocean between the models, and generally large differences compared to the observed temperature at intermediate depths. Warm bias models have a strong temperature anomaly of inflow of the Atlantic Water entering the Arctic Ocean through the Fram Strait. Another process that is not represented accurately in the CORE-II models is the formation of cold and dense water, originating on the eastern shelves. In the cold bias models, excessive cold water forms in the Barents Sea and spreads into the Arctic Ocean through the St. Anna Through. There is a large spread in the simulated mean heat and volume transports through the Fram Strait and the Barents Sea Opening. The models agree more on the decadal variability, to a large degree dictated by the common atmospheric forcing. We conclude that the CORE-II model study helps us to understand the crucial biases in the Arctic Ocean. The current coarse resolution state-of-the-art ocean models need to be improved in accurate representation of the Atlantic Water inflow into the Arctic and density currents coming from the shelves.},
  file = {C\:\\Users\\Jon\\Zotero\\storage\\3WFDBMBJ\\Ilıcak et al. - 2016 - An assessment of the Arctic Ocean in a suite of in.pdf;C\:\\Users\\Jon\\Zotero\\storage\\3YVD4LW5\\S1463500316000238.html},
  journal = {Ocean Modelling},
  keywords = {Arctic Ocean,Atlantic Water,CORE-II atmospheric forcing,Density currents,St. Anna Trough}
}

@book{imeson1973,
  title = {Landscape Stabilit\.y in the Luxembourg Ardennes as Exemplified by Hydrological and (Micro)Pedological Investigations of a Catena in an Experimental Watershed},
  author = {Imeson, A. C. and Jungerius, P. D.},
  year = {1973},
  doi = {10.1016/S0341-8162(73)80016-5},
  abstract = {The runoff in the Birbaach catchment conforms to the partial area contributions theory of storm runoff, and overland flow and saturation throughflow away from the river channel are unlikely to occur in appreciable amounts. The catchment soils appear adjusted to transmit all the rainfall they receive and a steady state condition is visualized. A high soil aggregate stability contributes to the resistance against erosion. This is particularly true of forest soils but a lower degree of aggregate stability under agriculture is, to some extent, compensated for by the stoniness of the soil. This explains the low output of suspended solids measured in the stream channel. On the other hand, erosion pin measurements indicate that some erosion does occur through frost and rainsplash action, even beneath forest, but only where bare soil is exposed. The lack of erosion suggested by the hydrological conditions and erosion and soil aggregate stability data, is corroborated by the continuing presence of Holocene Cambisols even on the steepest slopes. Their A3 or B1 horizon has been used to indicate the degree of erosion. These horizons originate from homogenisation as could be demonstrated by micromorfological methods and by the distribution of volcanic minerals. Beneath forest, complete profiles point to a lack of erosion except where occasionally colluvium reflects a local disturbance of the soils following woodcutting. The stability of the forest soils is largely due to the high porosity of the surface soil and to the high aggregate stability induced by the impregnation of plasma with organic matter. On agricultural land the clearance of woodland was followed by some erosion but this was nowhere catastrophic and the results are only apparent in the landscape behind terraces on steep land. \textcopyright{} 1973.},
  isbn = {0341-8162}
}

@article{irving2018,
  title = {A High-Resolution Streamflow and Hydrological Metrics Dataset for Ecological Modeling Using a Regression Model},
  author = {Irving, Katie and Kuemmerlen, Mathias and Kiesel, Jens and Kakouei, Karan and Domisch, Sami and J{\"a}hnig, Sonja C.},
