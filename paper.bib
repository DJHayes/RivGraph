@article{bjerklie2003,
  title = {Evaluating the Potential for Measuring River Discharge from Space},
  author = {Bjerklie, David M. and Lawrence Dingman, S. and Vorosmarty, Charles J. and Bolster, Carl H. and Congalton, Russell G.},
