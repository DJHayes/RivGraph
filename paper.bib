  author = {Lantuit, Hugues and Overduin, Pier Paul and Couture, Nicole and Wetterich, Sebastian and Ar{\'e}, Felix and Atkinson, David and Brown, Jerry and Cherkashov, Georgy and Drozdov, Dmitry and Forbes, Donald Lawrence and {Graves-Gaylord}, Allison and Grigoriev, Mikhail and Hubberten, Hans-Wolfgang and Jordan, James and Jorgenson, Torre and {\O}deg{\aa}rd, Rune Strand and Ogorodov, Stanislav and Pollard, Wayne H. and Rachold, Volker and Sedenko, Sergey and Solomon, Steve and Steenhuisen, Frits and Streletskaya, Irina and Vasiliev, Alexander},
