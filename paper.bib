@article{koussis2009,
  title = {Assessment and Review of the Hydraulics of Storage Flood Routing 70 Years after the Presentation of the {{Muskingum}} Method},
  author = {Koussis, Antonis D.},
  year = {2009},
  month = feb,
  volume = {54},
  pages = {43--61},
  issn = {0262-6667, 2150-3435},
  doi = {10.1623/hysj.54.1.43},
  abstract = {This work critically assesses the storage, or hydrological methods of flood routing, focusing on the Muskingum and Kalinin-Miljukov methods. The common hydraulic basis of these methods and their interrelationships are established, emphasising hydraulic derivations of the Muskingum method's weighting coefficient. Important characteristics of the routing scheme are highlighted, especially the scheme's affinity to the pseudo-viscosity method of shock computation; the flow-dependence (nonlinearity) of routing parameters is analysed, as are mass balance errors. Options in calculating depths and in handling lateral flows are presented. Storage routing models are shown to be instances of a numerically equivalent convection\textendash diffusion equation or kinematic wave-derived convection\textendash diffusion routing model that is also able to relate depth and discharge at-a-section via loop-shaped rating curves; the consequences of ignoring the rating curves' transience are pointed out. System analytic parameter estimations are summarised, a routing option with direct use of the diffusion wave system response function (SRF) is reviewed, the selection of routing reaches based on the river morphology is discussed, and the extension of storage-type routing to mass transport simulation is indicated.},
  file = {C\:\\Users\\Jon\\Zotero\\storage\\XIUY97WG\\Koussis - 2009 - Assessment and review of the hydraulics of storage.pdf},
  journal = {Hydrological Sciences Journal},
  language = {en},
  number = {1}
}

