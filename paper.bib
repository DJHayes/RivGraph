  author = {Meier, Walter N. and Hovelsrud, Greta K. and van Oort, Bob E. H. and Key, Jeffrey R. and Kovacs, Kit M. and Michel, Christine and Haas, Christian and Granskog, Mats A. and Gerland, Sebastian and Perovich, Donald K. and Makshtas, Alexander and Reist, James D.},
  year = {2014},
