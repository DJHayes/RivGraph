@article{sandeep2018,
  title = {Decline and Poleward Shift in {{Indian}} Summer Monsoon Synoptic Activity in a Warming Climate},
  author = {Sandeep, S. and Ajayamohan, R. S. and Boos, William R. and Sabin, T. P. and Praveen, V.},
  year = {2018},
  month = mar,
  volume = {115},
