@article{oneal2010,
  title = {The Rates and Spatial Patterns of Annual Riverbank Erosion Revealed through Terrestrial Laser-Scanner Surveys of the {{South River}}, {{Virginia}}},
  author = {O'Neal, Michael A. and Pizzuto, James E.},
  year = {2010},
  volume = {36},
  pages = {695--701},
  issn = {01979337},
  doi = {10.1002/esp.2098},
  abstract = {Abstract Between a.d. 2006 and 2008, we completed annual surveys of\$\textbackslash backslash\$ntwo mercury-contaminated eroding banks, one forested and the other\$\textbackslash backslash\$ngrass covered, along the gravel-bed, bedrock South River in Virginia.\$\textbackslash backslash\$nGridded digital terrain models with a resolution of 0{$\cdot$}05\dbend??m were\$\textbackslash backslash\$ncreated from bank topography data collected using a terrestrial laser\$\textbackslash backslash\$nscanner. Model comparisons indicate that the forested bank retreated\$\textbackslash backslash\$nnearly 1\dbend??m around two leaning trees, while elsewhere the extent\$\textbackslash backslash\$nof bank retreat was negligible. On the grassy bank, retreat was controlled\$\textbackslash backslash\$nby the creation of small overhanging clumps of turf at the top of\$\textbackslash backslash\$nthe bank, their occasional failure, and the ultimate removal of failed\$\textbackslash backslash\$ndebris from the bank toe. Partial autocorrelation analysis of vertically\$\textbackslash backslash\$nintegrated bank retreat demonstrates that bank profile erosion is\$\textbackslash backslash\$nvirtually uncorrelated at horizontal distances greater than about\$\textbackslash backslash\$n1\dbend??m on both banks, a length scale of approximately half the bank\$\textbackslash backslash\$nheight. This extensive streamwise variability suggests that widely\$\textbackslash backslash\$nspaced profile data cannot adequately represent bank erosion at these\$\textbackslash backslash\$nsites. Additional analysis of our comprehensive spatial data also\$\textbackslash backslash\$nindicates that traditional bank profile surveys with any spacing\$\textbackslash backslash\$ngreater than 1\dbend??m would result in measurement errors exceeding 10\%,\$\textbackslash backslash\$nan important conclusion for assessing annual rates of mercury loading\$\textbackslash backslash\$ninto the South River from bank erosion. Our results suggest that\$\textbackslash backslash\$nthree-dimensional gridded bare-earth models of bank topography may\$\textbackslash backslash\$nbe required to accurately measure annual bank retreat in similar\$\textbackslash backslash\$nriver systems. Copyright \textcopyright{} 2010 John Wiley \& Sons, Ltd.},
  journal = {Earth Surface Processes and Landforms},
  keywords = {Bank retreat,Digital terrain model,LiDAR,River erosion},
  number = {5}
}

