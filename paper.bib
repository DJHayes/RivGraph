  issn = {18667538},
  doi = {10.1007/s12517-014-1594-5},
  abstract = {Lateral movement of river channel within its valley with time is a\$\textbackslash backslash\$nuniversal phenomenon. Various processes of fluvio-geomorphological\$\textbackslash backslash\$nenvironment such as rapid valley fill deposition due to very low channel\$\textbackslash backslash\$ngradient and resultant migration of active channel, bank failure and\$\textbackslash backslash\$nbankline migration, frequent occurrence of floods, capturing of lower\$\textbackslash backslash\$ncourses of the tributaries by the trunk streams, and abandonment of\$\textbackslash backslash\$ntracks and avulsion are the most prominent factors behind channel\$\textbackslash backslash\$ndynamics in the sub-Himalayan part of West Bengal. Additionally, human\$\textbackslash backslash\$ninterventions in the form of engineering constructions (bridges, flood\$\textbackslash backslash\$nembankments, etc.), tea plantation, mining and quarrying, and\$\textbackslash backslash\$nagricultural encroachment have emerged as another dominant issue which\$\textbackslash backslash\$nis pulverizing the ways of natural channel adjustment as well as also\$\textbackslash backslash\$nthe channel behaviors. Both these processes are carrying significant\$\textbackslash backslash\$nmessages of immense vulnerability for the study area known for its\$\textbackslash backslash\$ngeomorphological instabilities. The dynamics of channel adjustment in\$\textbackslash backslash\$nterms of lateral migration can be lucidly illustrated through the\$\textbackslash backslash\$napplication of modern tools of geoinformatics, i.e., the techniques of\$\textbackslash backslash\$nremote sensing and geographic information system (GIS) which is widely\$\textbackslash backslash\$ncapable of detecting as well as representing the changes that took place\$\textbackslash backslash\$nover space and time. A meticulous endeavor has been made to assess the\$\textbackslash backslash\$nchannel dynamics of the Diana River of this concerned region for an\$\textbackslash backslash\$n85-year spanning period to unveil the nature of channel migration in\$\textbackslash backslash\$nterms of changes in historical bankline and centerline positions and\$\textbackslash backslash\$nalterations in meander geometries through numeric and graphical methods\$\textbackslash backslash\$ncoupled with field observations to define the overall zone of channel\$\textbackslash backslash\$nmigration for three subsequent reaches of the Diana River.},
  journal = {Arabian Journal of Geosciences},
  keywords = {Bankline migration,Centerline,CMZ,Diana River,GIS,River capture},
  number = {8}
}

@article{chapin2006,
  title = {Policy Strategies to Address Sustainability of {{Alaskan}} Boreal Forests in Response to a Directionally Changing Climate},
  author = {Chapin, F. Stuart and Lovecraft, Amy L. and Zavaleta, Erika S. and Nelson, Joanna and Robards, Martin D. and Kofinas, Gary P. and Trainor, Sarah F. and Peterson, Garry D. and Huntington, Henry P. and Naylor, Rosamond L.},
  year = {2006},
  month = nov,
  volume = {103},
  pages = {16637--16643},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.0606955103},
  abstract = {Human activities are altering many factors that determine the fundamental properties of ecological and social systems. Is sustainability a realistic goal in a world in which many key process controls are directionally changing? To address this issue, we integrate several disparate sources of theory to address sustainability in directionally changing social\textendash ecological systems, apply this framework to climate-warming impacts in Interior Alaska, and describe a suite of policy strategies that emerge from these analyses. Climate warming in Interior Alaska has profoundly affected factors that influence landscape processes (climate regulation and disturbance spread) and natural hazards, but has only indirectly influenced ecosystem goods such as food, water, and wood that receive most management attention. Warming has reduced cultural services provided by ecosystems, leading to some of the few institutional responses that directly address the causes of climate warming, e.g., indigenous initiatives to the Arctic Council. Four broad policy strategies emerge: (i) enhancing human adaptability through learning and innovation in the context of changes occurring at multiple scales; (ii) increasing resilience by strengthening negative (stabilizing) feedbacks that buffer the system from change and increasing options for adaptation through biological, cultural, and economic diversity; (iii) reducing vulnerability by strengthening institutions that link the high-latitude impacts of climate warming to their low-latitude causes; and (iv) facilitating transformation to new, potentially more beneficial states by taking advantage of opportunities created by crisis. Each strategy provides societal benefits, and we suggest that all of them be pursued simultaneously.},
  copyright = {\textcopyright{} 2006 by The National Academy of Sciences of the USA},
  file = {C\:\\Users\\Jon\\Zotero\\storage\\4FXRF8AA\\Chapin et al. - 2006 - Policy strategies to address sustainability of Ala.pdf;C\:\\Users\\Jon\\Zotero\\storage\\BMYA3GHG\\16637.html},
  journal = {Proceedings of the National Academy of Sciences},
  keywords = {adaptability,Alaska,climate change,resilience,vulnerability},
  language = {en},
  number = {45},
  pmid = {17008403}
}

