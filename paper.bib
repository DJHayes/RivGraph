@article{itkin2015,
  title = {Landfast Ice Affects the Stability of the {{Arctic}} Halocline: {{Evidence}} from a Numerical Model},
  shorttitle = {Landfast Ice Affects the Stability of the {{Arctic}} Halocline},
  author = {Itkin, Polona and Losch, Martin and Gerdes, R{\"u}diger},
  year = {2015},
