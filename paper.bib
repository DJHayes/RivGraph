@book{obu2018,
  title = {Ground {{Temperature Map}}, 2000-2016, {{Northern Hemisphere Permafrost}}},
  author = {Obu, Jaroslav and Westermann, Sebastian and K{\"a}{\"a}b, Andreas and Bartsch, Annett},
  year = {2018},
  publisher = {{PANGAEA}},
  doi = {10.1594/PANGAEA.888600},
  abstract = {The product provides modeled mean annual ground temperatures (MAGT) at the top of the permafrost for the Northern Hemisphere at 1 km spatial resolution. Permafrost probability (fraction values from 0 to 1) is assigned to each grid cell with MAGT \textbackslash textless 0C. Based on its permafrost probability each grid cell is classified as continuous, discontinuous and sporadic permafrost. The processing extent covers exposed land areas of the Northern Hemisphere down to 25 latitude. The mean MAGT was validated with GTN-P and TSP borehole ground temperature data and yielded a RMS of 2.0 C. According to the results, permafrost (MAGT \textbackslash textless 0 C) covers 15 \% of exposed land of the Northern Hemisphere.\$\textbackslash backslash\$\$\textbackslash backslash\$ Following files are provided in geotiff format:\$\textbackslash backslash\$\$\textbackslash backslash\$ - Mean Annual Ground Temperature (MAGTM) [C]\$\textbackslash backslash\$\$\textbackslash backslash\$ - MAGT Standard Deviation (MAGTSTD) [C]\$\textbackslash backslash\$\$\textbackslash backslash\$ - Permafrost Probability Fraction (PERPROB) [Fraction, 0-1]\$\textbackslash backslash\$\$\textbackslash backslash\$ Permafrost Zonation (PERZONES) is provided as ESRI shapefile.\$\textbackslash backslash\$\$\textbackslash backslash\$ All files are provided in Arctic Polar Stereographic projection (EPSG:3995 WGS 84).\$\textbackslash backslash\$\$\textbackslash backslash\$ More Information about the modelling method can be found in the product guide.\$\textbackslash backslash\$\$\textbackslash backslash\$ The map was produced within the project ESA Data User Element GlobPermafrost.\$\textbackslash backslash\$\$\textbackslash backslash\$ Dataset update: 2019-04-01}
}

@article{obu2019,
  title = {Northern {{Hemisphere}} Permafrost Map Based on {{TTOP}} Modelling for 2000\textendash 2016 at 1 Km 2 Scale},
  author = {Obu, Jaroslav and Westermann, Sebastian and Bartsch, Annett and Berdnikov, Nikolai and Christiansen, Hanne H. and Dashtseren, Avirmed and Delaloye, Reynald and Elberling, Bo and Etzelm{\"u}ller, Bernd and Kholodov, Alexander and Khomutov, Artem and K{\"a}{\"a}b, Andreas and Leibman, Marina O. and Lewkowicz, Antoni G. and Panda, Santosh K. and Romanovsky, Vladimir and Way, Robert G. and {Westergaard-Nielsen}, Andreas and Wu, Tonghua and Yamkhin, Jambaljav and Zou, Defu},
  year = {2019},
  volume = {193},
  pages = {299--316},
  issn = {00128252},
  doi = {10.1016/j.earscirev.2019.04.023},
  abstract = {Permafrost is a key element of the cryosphere and an essential climate variable in the Global Climate Observing System. There is no remote-sensing method available to reliably monitor the permafrost thermal state. To estimate permafrost distribution at a hemispheric scale, we employ an equilibrium state model for the temperature at the top of the permafrost (TTOP model) for the 2000\textendash 2016 period, driven by remotely-sensed land surface temperatures, down-scaled ERA-Interim climate reanalysis data, tundra wetness classes and landcover map from the ESA Landcover Climate Change Initiative (CCI) project. Subgrid variability of ground temperatures due to snow and landcover variability is represented in the model using subpixel statistics. The results are validated against borehole measurements and reviewed regionally. The accuracy of the modelled mean annual ground temperature (MAGT) at the top of the permafrost is {$\pm$}2 \textdegree C when compared to permafrost borehole data. The modelled permafrost area (MAGT \textbackslash textless0 \textdegree C) covers 13.9 \texttimes{} 10 6 km 2 (ca. 15\% of the exposed land area), which is within the range or slightly below the average of previous estimates. The sum of all pixels having isolated patches, sporadic, discontinuous or continuous permafrost (permafrost probability \textbackslash textgreater0) is around 21 \texttimes{} 10 6 km 2 (22\% of exposed land area), which is approximately 2 \texttimes{} 10 6 km 2 less than estimated previously. Detailed comparisons at a regional scale show that the model performs well in sparsely vegetated tundra regions and mountains, but is less accurate in densely vegetated boreal spruce and larch forests.},
  journal = {Earth-Science Reviews},
  keywords = {Cryosphere,Essential climate variable,Frozen ground,Ground temperatures,Permafrost,Permafrost map,Remote sensing},
  number = {October 2018}
}

