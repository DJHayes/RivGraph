@article{osterkamp2006,
  title = {Warming of Permafrost in the {{Arctic National Wildlife Refuge}}, {{Alaska}}},
  author = {Osterkamp, T E and Jorgenson, J C},
  year = {2006},
  volume = {17},
  pages = {65--69},
  abstract = {Permafrost borehole temperatures were measured in 1985, 1998, and 2004 on Barter Island near the village of Kaktovik and in the Arctic National Wildlife Refuge (Arctic NWR) north of the Brooks Range. These measurements indicate that the century-long warming documented for the central and western Arctic has also occurred in the region near Barter Island and in the northern Arctic NWR. At Kaktovik, the warming occurred during the second quarter of the 20th century or earlier and its magnitude exceeded 0.8{$\lnot\infty$}C. A more recent warming (since the mid to late 1980s) similar to that in the central and western Arctic is also occurring on Barter Island and in the northern Arctic NWR. Near Kaktovik, the permafrost warmed about 2 to 3{$\lnot\infty$}C from 1985 to 2004. In the northern Arctic NWR, on a line extending southward from Tapkaurak Point, it warmed about 1.5 to 2{$\lnot\infty$}C from 1985 to 1998. If air temperatures warm 5{$\lnot\infty$}C over the next century, as predicted, some of the permafrost in the northern Arctic NWR would be expected to thaw. Copyright {$\lnot$}\textcopyright{} 2005 John Wiley \& Sons, Ltd.},
  journal = {Permafrost and Periglacial Processes},
  number = {1}
}

