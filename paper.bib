@article{rodell2004,
  title = {The {{Global Land Data Assimilation System}}},
  author = {Rodell, M. and Houser, P. R. and Jambor, U. and Gottschalck, J. and Mitchell, K. and Meng, C.-J. and Arsenault, K. and Cosgrove, B. and Radakovich, J. and Bosilovich, M. and Entin, J. K. and Walker, J. P. and Lohmann, D. and Toll, D.},
  year = {2004},
  month = mar,
  volume = {85},
  pages = {381--394},
  issn = {0003-0007},
  doi = {10.1175/BAMS-85-3-381},
  abstract = {A Global Land Data Assimilation System (GLDAS) has been developed. Its purpose is to ingest satellite- and ground-based observational data products, using advanced land surface modeling and data assimilation techniques, in order to generate optimal fields of land surface states and fluxes. GLDAS is unique in that it is an uncoupled land surface modeling system that drives multiple models, integrates a huge quantity of observation-based data, runs globally at high resolution (0.25\textdegree ), and produces results in near\textendash real time (typically within 48 h of the present). GLDAS is also a test bed for innovative modeling and assimilation capabilities. A vegetation-based ``tiling'' approach is used to simulate subgrid-scale variability, with a 1-km global vegetation dataset as its basis. Soil and elevation parameters are based on high-resolution global datasets. Observation-based precipitation and downward radiation and output fields from the best available global coupled atmospheric data assimilation systems are employed as forcing data. The high-quality, global land surface fields provided by GLDAS will be used to initialize weather and climate prediction models and will promote various hydrometeorological studies and applications. The ongoing GLDAS archive (started in 2001) of modeled and observed, global, surface meteorological data, parameter maps, and output is publicly available.},
  file = {C\:\\Users\\Jon\\Zotero\\storage\\ATTM22YK\\Rodell et al. - 2004 - The Global Land Data Assimilation System.pdf;C\:\\Users\\Jon\\Zotero\\storage\\ZW576LGI\\BAMS-85-3-381.html},
  journal = {Bulletin of the American Meteorological Society},
  number = {3}
}

@article{rodnight2005,
  title = {Optical Dating of a Scroll-Bar Sequence on the {{Klip River}}, {{South Africa}}, to Derive the Lateral Migration Rate of a Meander Bend},
  author = {Rodnight, H. and Duller, G. A T and Tooth, S. and Wintle, A. G.},
  year = {2005},
  volume = {15},
  pages = {802--811},
  issn = {09596836},
  doi = {10.1191/0959683605hl854ra},
  abstract = {Problems are often encountered in accurately establishing the rates of processes associated with river channel change. In this study, optical dating was used to obtain the lateral migration rate of an abandoned meander bend on the Klip River, eastern Free State, South Africa. Five samples were collected by augering along a transect across an approximately 200-m-wide scroll-bar sequence on the inner meander bend, and an additional sample was collected from the channel fill of the related oxbow lake. Optically stimulated luminescence (OSL) ages were derived for each sample using the single-aliquot regenerative-dose protocol on small aliquots of quartz. Incomplete bleaching was evident in some of the samples and thus the appropriate statistical analysis was performed using the 'finite mixture model' for the scroll-bar samples and the 'minimum age model' for the channel-fill sample. The ages calculated were in the correct chronological order for the depositional sequence, which was formed over the last \&sim; 1000 years. The average rate of lateral migration across the scroll-bar sequence was found to be \&sim; 0.16 m/yr but with an apparent increase in the rate occurring during the early sixteenth century. Where quartz-rich alluvium exists, the methods used in this study can potentially be applied to other river systems, enabling rates of channel change processes to be determined. (c) 2005 Edward Arnold (Publishers) Ltd.},
  journal = {Holocene},
  keywords = {Klip River,Late Holocene,Lateral migration,Luminescence dating,Meander,Optical dating,Scroll bar,South Africa},
  number = {6}
}

