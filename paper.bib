@article{lenton2008,
  title = {Tipping Elements in the {{Earth}}'s Climate System},
  author = {Lenton, Timothy M. and Held, Hermann and Kriegler, Elmar and Hall, Jim W. and Lucht, Wolfgang and Rahmstorf, Stefan and Schellnhuber, Hans Joachim},
