@article{chakraborty2015,
  title = {An Assessment on the Nature of Channel Migration of {{River Diana}} of the Sub-{{Himalayan West Bengal}} Using Field and {{GIS}} Techniques},
  author = {Chakraborty, Subhankar and Mukhopadhyay, Sutapa},
  year = {2015},
  volume = {8},
