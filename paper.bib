@article{duffy1999,
  title = {Sensitivity of Simulated Salinities in a Three-Dimensional Ocean General Circulation Model to Vertical Mixing of Destabilizing Surface Fluxes},
  author = {Duffy, P. B. and Caldeira, K. G.},
  year = {1999},
  month = jan,
